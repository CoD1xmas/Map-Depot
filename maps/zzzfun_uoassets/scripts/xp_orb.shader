skins/xp_orb_red
{
	surfaceparm noncolliding
	surfaceparm trans
	DeformVertexes autosprite
	deformVertexes wave 32 sin 0 2 0 .75
	{
		map skins/xp_orb_red.tga
		rgbGen identityLighting
		alphaGen const 1
		blendFunc blend
	}
}

